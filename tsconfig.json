{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "esnext",
        "jsx": "preserve",
        "strict": true,
        "noEmit": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "allowJs": true,
        "lib": ["dom", "esnext", "es2015", "es2017"]
    },
    "include": [
        "./src/**/*",
        "./gatsby-node.js"
    ],
}